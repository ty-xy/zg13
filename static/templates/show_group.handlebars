            </div>
            <button class="new_setting">新建群组</button>
        </div>
    </div>
    <div class="streams-list">
        {{#each subscriptions}}
        <div class="stream-row stream-list-row" data-stream-id="{{stream_id}}" data-stream-name="{{name}}">
