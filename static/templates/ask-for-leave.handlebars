<div class="ask-for-leave-title">
  <span class="first-icon"><i class="iconfont icon-xiayiji "></i></span>
  <span>请假申请</span>
</div>
<div class="container">
    <br>
        <form class="form-horizontal" role="form" id="form-test">
            <div class="form-group form-group-common ">
                <label for="username" class="col-sm-1 control-label control-labels">请假人:</label>
                <div class="col-md-12">
                    <input type="text" class="form-control form-people" id="username" name="username" placeholder="请输入...">
                </div>
            </div>
            <div class="form-group form-group-common">
                <label for="newplan_datetimepicker1" class="col-sm-1 control-label control-labels">开始日期:</label>
                 <div class="input-append date" id="newplan_datetimepicker1" data-date="" data-date-format="yyyy-mm-dd">  
                            <input class="new_task_date create_taskdate form-people new-leave-date start_times" size="16" type="text" value="" readonly placeholder="请选择日期...">  
                            <span class="add-on select_date_icon select-icons">
                                <i data-date-icon="icon-calendar" class="icon-calendar iconfont icon-riqi icon-riqis"></i>
                            </span>  
                  </div> 
            </div>
            <div class="form-group  form-group-common">
                <label for="newplan_datetimepicker2" class="col-sm-1 control-label control-labels">结束日期:</label>
                  <div class="input-append date" id="newplan_datetimepicker2" data-date="" data-date-format="yyyy-mm-dd">  
                            <input class="new_task_date create_taskdate form-people new-leave-date end_times" size="16" type="text" value="" readonly placeholder="请选择日期...">  
                            <span class="add-on select_date_icon select-icons">
                                <i data-date-icon="icon-calendar" class="icon-calendar iconfont icon-riqi icon-riqis"></i>
                            </span>  
                  </div> 
            </div>
            <div class="form-group form-group-common">
                <label for="email" class="col-sm-1 control-label control-labels">请假天数:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control form-people" id="email" name="email" placeholder="请输入...">
                </div>
            </div>
             <div class="form-group form-group-common">
                <label for="text" class="col-sm-1 control-label control-labels ">请假事由:</label>
                <div class="col-sm-10">
                    <textarea type="text" class="form-control  form-people  form-reason " id="text"  placeholder="请输入请假事由…"></textarea>
                </div>
            </div> 
           <div class="form-group form-group-common form-group-img">
               <div class="control-labels control-img">添加图片:</div>
               <div class="img-border">
                   <i class="iconfont icon-tianjia3 add-img-people"></i>
               </div>
           </div>
           <div class="form-group form-group-common">
                <div class="control-labels control-img">审批人:</div>
                <ul class="generate_log_member_box shenpi-person check-people">
                    <li class="add_log_people">
                        <div class="generate_log_member_addlogos">
                           <i class="iconfont icon-tianjia3 add-img-people icon-position"></i>
                        </div>
                        <p class="generate_log_member_adds">选择成员</p>
                    </li>
                </ul>
           </div>
            <div class="form-group form-group-common">
                <div class="control-labels control-img">抄送人:</div>
                <ul class="generate_log_member_box shenpi-person send-check-people">
                    <li class="add_log_peoples">
                        <div class="generate_log_member_addlogos">
                           <i class="iconfont icon-tianjia3 add-img-people icon-position"></i>
                        </div>
                        <p class="generate_log_member_adds">选择成员</p>
                    </li>
                </ul>
           </div>
            <div class="form-group form-group-common">
                <div class="col-sm-offset-4 col-sm-10 ">
                    <button type="submit" class="btn btn-primary btn-submit" id="btn-test">提交</button>
                </div>
            </div>
        </form>
</div>




