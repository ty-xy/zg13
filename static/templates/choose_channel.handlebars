<div class="icon-sousuo-search">
                <i class="iconfont icon-sousuo icon-search-choose"></i>
                <input placeholder="搜索" class="search-icon"/>
            </div>
            <div class="input-box">
               <input type="checkbox" class="checkbox-input"/>
               <span>全选</span>
            </div>
            <ul class="box-left-list">
                {{#each data}}
                 <li class="input-box-list">
                    <div class="box-list-left">
                        <input type="checkbox"  inputid={{@key}} class="choose-check"/>
                        <span>{{@key}}</span>
                    </div>
                    <button class="button-right next-right" button-key={{@key}}>下级</button>
                </li>
                 {{/each}}
              
</ul>